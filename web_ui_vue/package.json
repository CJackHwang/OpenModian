{
  "name": "modian-spider-vue-ui",
  "version": "1.0.0",
  "description": "摩点爬虫管理系统 - Vue3 + Material Design 3 前端界面",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview --host",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "vue-tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:deps": "rm -rf node_modules/.vite && npm run dev",
    "dev:clean": "rm -rf node_modules/.vite && npm run dev",
    "serve": "vite preview --port 3001 --host"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@vueuse/core": "^13.3.0",
    "axios": "^1.10.0",
    "chart.js": "^4.5.0",
    "dayjs": "^1.11.13",
    "pinia": "^3.0.3",
    "socket.io-client": "^4.8.0",
    "vue": "^3.5.16",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1",
    "vuetify": "^3.8.9"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "^9.28.0",
    "prettier": "^3.3.0",
    "sass": "^1.80.0",
    "vite": "^6.3.5",
    "vite-plugin-vuetify": "^2.0.0",
    "vite-bundle-analyzer": "^0.11.0",
    "@types/node": "^22.0.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
